{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "ENV": true,
    "Velocity": true,
    "hypertranscript": true,
    "ActiveXObject": true,
    "hyperplayer": true,
    "process": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    // "indent": [
    //   "error",
    //   2
    // ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
