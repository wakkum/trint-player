{
  "name": "trint-player",
  "version": "0.0.1",
  "description": "Trint Interactive Transcript Player",
  "bugs": {
    "url": "https://github.com/trint-it/trint-player/issues"
  },
  "main": "lib/trint-player.js",
  "scripts": {
    "dev": "rollup -c --watch --input=src/trint-player.js --output=static/trint-player.js",
